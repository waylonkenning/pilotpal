{
  "name": "ppl-quest-nz",
  "version": "1.0.0",
  "description": "Gamified PPL training tracker for New Zealand",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:production": "BASE_URL=https://pilotpal-two.vercel.app playwright test",
    "test:type": "vue-tsc --noEmit",
    "test:build": "npm run test:type && npm run build",
    "test:all": "npm run test:type && npm run lint && npm run test && npm run test:e2e",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@types/leaflet": "^1.9.19",
    "leaflet": "^1.9.4",
    "pinia": "^2.1.7",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.2",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^26.1.0",
    "prettier": "^3.1.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^1.8.25"
  }
}
